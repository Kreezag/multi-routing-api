{
  "name": "redux-routed-api-middleware",
  "version": "1.0.0",
  "description": "redux api middleware for separate your api routes, api actions, and interceptors. provide state normalization, db reducer",
  "main": "lib/index.js",
  "scripts": {
    "cleanup": "npm prune && npm i && npm dedupe",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aliaksandr-master/redux-routed-api-middleware.git"
  },
  "keywords": [
    "redux",
    "middleware",
    "fetch",
    "api",
    "routes",
    "action",
    "actions",
    "server",
    "normalizr",
    "normalization",
    "state",
    "client",
    "db"
  ],
  "author": "alxe.master@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aliaksandr-master/redux-routed-api-middleware/issues"
  },
  "homepage": "https://github.com/aliaksandr-master/redux-routed-api-middleware#readme",
  "devDependencies": {
    "babel-core": "6.21.0",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-es2016": "6.16.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-2": "6.18.0",
    "babel-register": "6.18.0",
    "babel-runtime": "6.20.0",
    "eslint": "3.12.2",
    "eslint-config-airbnb": "13.0.0",
    "eslint-config-eslint": "3.0.0",
    "eslint-config-standard": "6.2.1",
    "eslint-import-resolver-webpack": "0.8.0",
    "eslint-loader": "1.6.1",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "3.0.2",
    "eslint-plugin-promise": "3.4.0",
    "eslint-plugin-react": "6.9.0",
    "eslint-plugin-require-path-exists": "1.1.5",
    "eslint-plugin-standard": "2.0.1",
    "husky": "0.12.0",
    "jest": "18.1.0",
    "jest-cli": "18.1.0",
    "webpack": "1.14.0"
  },
  "dependencies": {
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-polyfill": "6.20.0",
    "fetch-jsonp": "1.0.5",
    "localforage": "1.4.3",
    "normalizr": "3.1.0",
    "urijs": "1.18.4",
    "usymbol": "1.0.4",
    "whatwg-fetch": "2.0.1"
  },
  "jest": {
    "automock": false,
    "notify": true,
    "testEnvironment": "jsdom",
    "cacheDirectory": ".tmp/jest-cache",
    "coverageDirectory": ".tmp/tests-coverage-report",
    "collectCoverageFrom": [
      "**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/"
    ],
    "moduleFileExtensions": [
      "",
      "js",
      "jsx"
    ],
    "testRegex": "/__tests__/.+?\\.test\\.js$",
    "moduleDirectories": [
      "node_modules"
    ],
    "verbose": false
  }
}
